{
  "manifest_version": 2,

  "name": "ptt-post-tracker",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click HERE"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "<all_urls>",
    "storage"
  ]
}
